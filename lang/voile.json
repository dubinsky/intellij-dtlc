{
	"languageName": "Voile",
	"constantPrefix": "VOILE",
	"exeName": "voilec",
	"runConfigInit": "",
	"trimVersion": "version.substring(\"voilec\".length()).trim()",
	"supportsParsing": true,
	"website": "https://docs.rs/voile",
	"highlightTokenPairs": {
		"KEYWORD": "KEYWORD",
		"IDENTIFIER": "IDENTIFIER",
		"COMMA": "COMMA",
		"PAREN": "PARENTHESES",
		"SEMICOLON": "SEMICOLON",
		"BRACE": "BRACES",
		"LINE_COMMENT": "LINE_COMMENT",
		"FUNCTION_NAME": "FUNCTION_DECLARATION",
		"CONSTRUCTOR": "FUNCTION_DECLARATION",
		"UNRESOLVED": "IDENTIFIER",
		"OPERATOR": "OPERATION_SIGN",
		"BRACE2": "BRACES"
	},
	"keywordList": ["Sum", "Rec", "Type", "let", "val", "case", "or", "whatever"],
	"braceTokenPairs": {
		"LBRACE": "RBRACE",
		"LBRACE2": "RBRACE2",
		"LPAREN": "RPAREN"
	},
	"findUsagesProvider": "WithoutString"
}