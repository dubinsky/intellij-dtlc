{
	"languageName": "Narc",
	"constantPrefix": "NARC",
	"exeName": "narc",
	"runConfigInit": "",
	"trimVersion": "version.substring(\"narc\".length()).trim()",
	"supportsParsing": true,
	"website": "https://docs.rs/nar",
	"highlightTokenPairs": {
		"KEYWORD": "KEYWORD",
		"IDENTIFIER": "IDENTIFIER",
		"PAREN": "PARENTHESES",
		"SEMICOLON": "SEMICOLON",
		"BRACE": "BRACES",
		"LINE_COMMENT": "LINE_COMMENT",
		"FUNCTION_NAME": "FUNCTION_DECLARATION",
		"UNRESOLVED": "IDENTIFIER",
		"OPERATOR": "OPERATION_SIGN",
		"INACCESS": "BRACES"
	},
	"keywordList": [
		"data", "clause", "codata", "definition",
		"projection", "constructor", "Type"
	],
	"findUsagesProvider": "WithoutString",
	"declarationTypes": [
		{
			"name": "Definition",
			"identifierName": "NameDecl",
			"icon": "SemanticIcons.ORANGE_LAMBDA",
			"findType": "UtilsKt.findType(this, NarcTypes.COLON)"
		},
		{
			"name": "Data",
			"identifierName": "NameDecl",
			"icon": "SemanticIcons.BLUE_HOLE",
			"findType": "UtilsKt.findType(this, NarcTypes.COLON)"
		},
		{
			"name": "Constructor",
			"identifierName": "NameDecl",
			"icon": "SemanticIcons.BLUE_C",
			"findType": "UtilsKt.findType(this, NarcTypes.COLON)"
		},
		{
			"name": "Projection",
			"identifierName": "NameDecl",
			"icon": "SemanticIcons.PURPLE_P",
			"findType": "UtilsKt.findType(this, NarcTypes.COLON)"
		},
		{
			"name": "Codata",
			"identifierName": "NameDecl",
			"icon": "SemanticIcons.PINK_HOLE",
			"findType": "UtilsKt.findType(this, NarcTypes.COLON)"
		}
	],
	"braceTokenPairs": {
		"LBRACE": "RBRACE",
		"LINACCESS": "RINACCESS",
		"LPAREN": "RPAREN"
	}
}